make.basetree <- function(ntax) {
      vect = vector()
      for (i in 1:ntax) {
            if (i < 10) vector[i] <- paste("SE00",i,sep="")
            else if (100 > i & i >= 10) vector[i] <- paste("SE0",i,sep="")
            else if (1000 > i & i >= 100) vector[i] <- paste("SE",i,sep="")
      }
      return (rcoal(ntax,vector))
}
